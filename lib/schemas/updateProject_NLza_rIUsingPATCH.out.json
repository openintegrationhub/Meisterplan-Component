{
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "example": "0b9e0e29-8d87-4db8-baa8-1b1a49e354da"
        },
        "name": {
            "type": "string",
            "example": "Meisterplan API v1.1"
        },
        "externalId": {
            "type": "string",
            "example": "myCustomID"
        },
        "start": {
            "type": "string",
            "example": "2019-01-31",
            "description": "format: YYYY-MM-DD"
        },
        "finish": {
            "type": "string",
            "example": "2019-01-31",
            "description": "format: YYYY-MM-DD"
        },
        "manager": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "0b9e0e29-8d87-4db8-baa8-1b1a49e354da"
                }
            },
            "title": "ProjectManagerResponse"
        },
        "costType": {
            "type": "string",
            "example": "CAPEX",
            "description": "default: see Meisterplan documentation.",
            "enum": [
                "CAPEX",
                "OPEX"
            ]
        },
        "status": {
            "description": "default: see Meisterplan documentation.",
            "type": "object",
            "properties": {
                "value": {
                    "type": "string",
                    "example": "Approved",
                    "description": "If you have no permission to this field, the value is null."
                }
            },
            "title": "StatusFieldResponse"
        },
        "notes": {
            "type": "string"
        },
        "businessGoal": {
            "example": {
                "name": "Change the Business"
            },
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "example": "Change the Business"
                }
            },
            "title": "BusinessGoalResponse"
        },
        "approvedTotalEffort": {
            "example": {
                "hours": 104.5
            },
            "type": "object",
            "properties": {
                "hours": {
                    "type": "number",
                    "format": "double",
                    "example": 104.5
                }
            },
            "title": "ApprovedTotalEffortResponse"
        },
        "customFields": {
            "type": "object",
            "example": {
                "cust_stage_gate": {
                    "value": "Approved"
                },
                "cust_risk": {
                    "value": "Medium"
                }
            },
            "additionalProperties": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "object",
                        "description": "Value may be of type string, number, boolean, or null depending on the custom field definition and its current value."
                    }
                },
                "title": "CustomFieldResponse"
            }
        },
        "obsUnits": {
            "type": "object",
            "example": {
                "Department": {
                    "path": "Services/Consulting/IT",
                    "displayName": "Services > Consulting > IT"
                }
            },
            "additionalProperties": {
                "type": "object",
                "required": [
                    "displayName",
                    "path"
                ],
                "properties": {
                    "displayName": {
                        "type": "string",
                        "description": "The display name is rendered the same way as it is in the Meisterplan application's user interface"
                    },
                    "path": {
                        "type": "string"
                    }
                },
                "title": "ObsUnitResponse"
            }
        },
        "priority": {
            "type": "object",
            "properties": {
                "rankCategory": {
                    "type": "string",
                    "example": "REGULAR",
                    "enum": [
                        "ABOVE_MUST_HAVE",
                        "BELOW_CUT_OFF",
                        "REGULAR"
                    ]
                }
            },
            "title": "PriorityResponse"
        },
        "program": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "0b9e0e29-8d87-4db8-baa8-1b1a49e354da"
                }
            },
            "title": "ProgramLinkResponse"
        }
    }
}